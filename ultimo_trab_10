def seila(nome_arquivo):


    arquivo = open(nome_arquivo, 'r')
    for linha in arquivo:
        print(linha)
    arquivo.close()

# se o arquivo nao estiver na mesma pasta que seu arquivo.py
# colocar r'endere√ßo\nome', ex: 
#seila(r'C:\Users\Lucas\Desktop\Alou.txt')
seila('questao9.txt') #arquivo na mesma pasta
