import numpy as np

#Slicing
l = [20,30,10,40]
print(l[1:3]) #[30,10]
print(l[::2]) #[20,10]

a = np.array(l)
print(a[1:3]) #[30 10]
print(a[::2]) # [20 10]

l2 = l[:]
print(l2) # [ 20,30,10,40]

l2[0] = 1000
print(l2) # [1000,30,10,40]


b = a[:]
print(b) #[20,30,10,40]

b[0] = 1000
print(b) #[ 1000,30,10,40]

#Copiando
c = a.copy()
print(c)
