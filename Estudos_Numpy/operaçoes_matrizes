import numpy as np

m1 = np.array([[1,2,3],[4,5,6]])
m2 = np.array([[7,8,9],[10,11,12]])
print(m1)
print(m2)


# m2/m1 , divide os elementos em posi√ßoes correspondentes
print(m2/m1)

# m1* m2 multiplica os elementos
print(m1*m2)

# elevar a n todos os elementos da matriz
print(m1**2) # elevado a 2 por exemplo 
