def bi(a,b):
    c = a + b
    r = []
    if c == '00':
        r.append('False')
    elif c == '01':
        r.append('True')
    elif c == '10':
        r.append('True')
    elif c == '11':
        r.append('True')
    else:
        r.append("Voce não digitou um binario válido")

    return r

x = str (input("Digite o binário a:"))
y = str (input("Digite o binario b:"))

print(bi(x,y))
